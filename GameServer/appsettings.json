{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ListenAddress": "http://{listenIP}:{gamePort}",
    "ServerIP": "{serverIP}",
    "ConnectionStrings": {
        "DefaultConnection": "server={serverIP};port={mySQLPort};database=GameMySQL;user=root;password={myPassword};",
        "RedisConnection": "{serverIP}:{redisPort},password={redisPassword}"
    },
    "HiveServer": "{hiveAddr}"
}
