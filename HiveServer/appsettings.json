{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ListenAddress": "http://{listenIP}:{hivePort}",
  "ServerIP": "{serverIP}",
  "ConnectionStrings": {
    "DefaultConnection": "server={serverIP};port={mySQLPort};database=HiveMySQL;user=root;password={myPassword};",
    "RedisConnection": "{serverIP}:{redisPort},password={redisPassword}"
  },
  "Jwt": {
    "Key": "{jwtKey}",
    "Issuer": "HiveServer",
    "Audience": "HiveClient",
    "DurationInMinutes": 30
  }
}
