{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServerAddress": "{listenAddr}",
  "ConnectionStrings": {
    "DefaultConnection": "server={serverAddr};port={mySQLPort};database=HiveMySQL;user=root;password={myPassword};",
    "RedisConnection": "{serverAddr}:{redisPort}"
  },
  "Jwt": {
    "Key": "{jwtKey}",
    "Issuer": "HiveServer",
    "Audience": "HiveClient",
    "DurationInMinutes": 30
  }
}
